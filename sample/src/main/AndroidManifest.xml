<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.airwallex.paymentacceptance">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="com.airwallex.paymentacceptance.SampleApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.airwallex.paymentacceptance.PaymentCartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name="com.airwallex.paymentacceptance.PaymentStartPayActivity" />

        <activity android:name="com.airwallex.paymentacceptance.EditShippingActivity" />

        <activity android:name="com.airwallex.paymentacceptance.PaymentSelectMethodActivity" />

        <activity
            android:name="com.airwallex.paymentacceptance.wxapi.WXPayEntryActivity"
            android:exported="true"
            android:launchMode="singleTop">
        </activity>

        <receiver
            android:name="com.airwallex.paymentacceptance.AppRegister"
            android:permission="com.tencent.mm.plugin.permission.SEND" >
            <intent-filter>
                <action android:name="com.tencent.mm.plugin.openapi.Intent.ACTION_REFRESH_WXAPP" />
            </intent-filter>
        </receiver>

    </application>

</manifest>